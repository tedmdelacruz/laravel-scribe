{
  "private": true,
  "scripts": {
    "prod": "gulp --production",
    "dev": "gulp watch",
    "build": "browserify resources/assets/js/app.js -t babelify -o public/js/app.js",
    "watch": "watchify resources/assets/js/app.js -t babelify -o public/js/app.js --poll=100"
  },
  "transform": [
    [
      "babelify",
      {
        "comments": false
      }
    ]
  ],
  "devDependencies": {
    "babel-preset-es2015": "^6.9.0",
    "babelify": "^7.3.0",
    "bootstrap-sass": "^3.3.0",
    "gulp": "^3.9.1",
    "laravel-elixir": "^5.0.0",
    "watchify": "^3.7.0"
  }
}
